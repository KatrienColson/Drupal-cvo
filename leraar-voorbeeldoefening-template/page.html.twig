<header>
		<div class="container">
			<div class="row">
				<div class="col-md-4">
					{{ page.branding }}
				</div>
				<div class="col-md-8">
					<nav class="navbar navbar-expand-lg navbar-light">
					  
					  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
					    <span class="navbar-toggler-icon"></span>
					  </button>

					  <div class="collapse navbar-collapse" id="navbarSupportedContent">
					    {{ page.navigatie }}
					  </div>
					</nav>
				</div>		
			</div>
		</div>
	</header>

	<div class="indekijker">
		<div class="container">
			<div class="row">
				<div class="col-md-4">
					
						{{ page.indekijker1 }}
					
				</div>

				<div class="col-md-4">
					
						{{ page.indekijker2 }}
					
				</div>

				<div class="col-md-4">
					
						{{ page.indekijker3 }}
					
				</div>
			</div>
		</div>
	</div>

	<div class="maincontent">
		<div class="container">
			<div class="row">
				<div class="col-md-8">
					{{ page.content }}
				</div>
				<div class="col-md-4">
					{{ page.sidebar }}
				</div>
			</div>
		</div>
	</div>

	<div class="reclame">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					{{ page.reclame }}

				</div>
			</div>
		</div>
	</div>

	<footer>
		<div class="container">
			<div class="row">
				<div class="col-md-4">
					{{ page.footer1 }}
				</div>

				<div class="col-md-4">
					{{ page.footer2 }}
				</div>

				<div class="col-md-4">
					{{ page.footer3 }}
				</div>
			</div>
		</div>
	</footer>